# Quick edit `rc` files
alias zshrc="vim ~/.zshrc"
alias vimrc="vim ~/.vim/vimrc"

# Easier directory navigation
alias ~="cd ~"
alias -- -="cd -"

alias sub="filebot -get-subtitles"
alias rg="rg --type-add 'web:*{html,css,js}' --smart-case --follow"
alias reload="exec $SHELL -l"

eval "$(hub alias -s)"
