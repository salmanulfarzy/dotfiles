[alias]
    ss = status -s
    sb = status -b
    cmt = commit -v

    pl = pull --rebase
    p = rebase -i @{u}

    co = checkout
    diff = diff --word-diff
    dc = diff --cached

    ls = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --graph
    filelog = log -u

    la = "!git config -l | grep alias | cut -c 7-"
[user]
    email = salmanulfarzy@gmail.com
    name = Salmanul Farzy
[push]
    default = simple
[status]
    showUntrackedFiles = all
[core]
    excludesfile = /home/salman/.gitignore
