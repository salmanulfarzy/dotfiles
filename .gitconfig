[alias]
  s    = status -sb
  cmt  = commit -v
  cmsg = commit -m
  cane = commit --amend --no-edit

  pl = pull --rebase
  p  = rebase -i @{u}

  bt = branch
  co = checkout
  dc = diff --cached
  cp = cherry-pick
  ap = add -p

  last    = log -1 HEAD
  lo      = log --oneline -n 10
  dog     = log --decorate --oneline --graph
  adog    = log --all --decorate --oneline --graph
  filelog = log -u

  la = "!git config -l | grep alias | cut -c 7-"
[user]
  email      = salmanulfarzy@gmail.com
  name       = Salmanul Farzy
  signingkey = 7325421E81414002
  signingkey = 83CE707DA1CDC9D2
  signingkey = 6C326B61B424A736
[push]
  default = simple
[status]
  showUntrackedFiles = all
[core]
  autocrlf     = input
  excludesfile = ~/.gitignore
[commit]
  gpgsign = true
  cleanup = strip
[pager]
  branch = false
[mergetool]
  keepBackup = true
[color "branch"]
  curent = green reverse
  local  = green
  remote = yellow
[color "status"]
  added     = yellow
  changed   = green
  untracked = blue

[credential]
	helper = cache --timeout=600
