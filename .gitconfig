[alias]
	ss = status -s
	sb = status -b
	cmt = commit -v

	pl = pull --rebase
	p = rebase -i @{u}
	
    co = checkout
	diff = diff --word-diff
	dc = diff --cached

	lsds = log --pretty=format:"%C(yellow)%h\\ %ad%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --graph --date=short
	lsdr = log --pretty=format:"%C(yellow)%h\\ %ad%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --graph --date=relative
	ls = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --graph
	filelog = log -u

	la = "!git config -l | grep alias | cut -c 7-"
