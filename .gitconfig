[alias]
    s    = status -sb
    cmt  = commit -v
    cmsg = commit -m

    pl = pull --rebase
    p = rebase -i @{u}

    co = checkout
    dc = diff --cached
    cp = cherry-pick

    dog     = log --decorate --oneline --graph
    adog    = log --all --decorate --oneline --graph
    filelog = log -u

    la = "!git config -l | grep alias | cut -c 7-"
[user]
