    signingkey = 83CE707DA1CDC9D2
    signingkey = 6C326B61B424A736
[push]
    default = simple
[status]
    showUntrackedFiles = all
[core]
    pager = less -+F -+X
    excludesfile = ~/.gitignore
[commit]
    gpgsign = true
[pager]
    branch = false
[mergetool]
    keepBackup = true
[color "status"]
    added = yellow
    changed = green
    untracked = blue
